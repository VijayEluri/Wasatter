<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_height="fill_parent"
	android:layout_width="fill_parent">
	<LinearLayout android:layout_height="wrap_content" android:id="@+id/LinearLayout01" android:orientation="vertical" android:layout_width="fill_parent" android:layout_weight="1"><TextView android:layout_height="wrap_content" android:textSize="20sp" android:paddingBottom="10dip" android:paddingTop="10dip" android:paddingLeft="5dip" android:text="@string/setup_wizard_main_title" android:id="@+id/TextView01" android:layout_width="fill_parent"></TextView>
<TextView android:layout_height="wrap_content" android:textSize="16sp" android:paddingBottom="10dip" android:paddingLeft="5dip" android:text="@string/setup_wizard_main_text" android:id="@+id/wizardText" android:paddingRight="5dip" android:layout_width="fill_parent"></TextView>
<EditText android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_marginBottom="5dip" android:layout_marginTop="5dip" android:layout_marginLeft="10dip" android:layout_marginRight="10dip" android:inputType="text" android:hint="@string/setup_wizard_hint_wassr_id" android:id="@+id/wassrId"></EditText><Button android:layout_height="wrap_content" android:text="GotoButton" android:id="@+id/gotoButton" android:layout_width="fill_parent" android:layout_marginBottom="5dip" android:layout_marginLeft="10dip" android:layout_marginRight="10dip" android:layout_marginTop="5dip"></Button>

</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_height="wrap_content" android:layout_gravity="bottom"
		android:layout_width="fill_parent">
		<Button android:id="@+id/prevButton" android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="fill_parent" android:text="@string/setup_wizard_button_prev"></Button><Button android:text="@string/setup_wizard_button_next"
			android:layout_height="wrap_content" android:id="@+id/nextButton" android:layout_weight="1" android:layout_width="fill_parent"></Button>
	
</LinearLayout>

</LinearLayout>
